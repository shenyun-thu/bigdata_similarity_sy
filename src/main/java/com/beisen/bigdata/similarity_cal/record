CREATE EXTERNAL TABLE `talentSimilarity_eccompetenceuserresultinfo`(
  `rowkey` string COMMENT '',
  `similarity` string COMMENT '',
  )
ROW FORMAT SERDE
  'org.apache.hadoop.hive.hbase.HBaseSerDe'
STORED BY
  'org.apache.hadoop.hive.hbase.HBaseStorageHandler'
WITH SERDEPROPERTIES (
  'hbase.columns.mapping'=':key,fmy:similarity',
  'serialization.format'='1')
TBLPROPERTIES (
  'hbase.table.name'='beisendw:talentSimilarity_eccompetenceuserresultinfo')、
  
